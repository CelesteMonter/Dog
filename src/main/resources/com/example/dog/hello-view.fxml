<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<VBox alignment="CENTER" prefHeight="290.0" prefWidth="219.0" spacing="20.0" xmlns="http://javafx.com/javafx/16"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.dog.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <children>
        <Label text="Name"/>
        <TextField fx:id="nameField"/>

        <Label text="Raza"/>
        <TextField fx:id="breedField"/>

        <Label text="Altura"/>
        <TextField fx:id="heightField"/>

        <Label text="Peso"/>
        <TextField fx:id="weightField"/>

        <Label text="Genero"/>
        <TextField fx:id="genderField"/>

        <Label text="Edad"/>
        <TextField fx:id="ageField"/>

        <Button onAction="#setDog" text="Inscribir mascota"/>

        <TextArea minHeight="200" fx:id="textArea"/>
    </children>
</VBox>
